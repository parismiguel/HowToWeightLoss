<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Meta Tag -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <link rel="alternate" type="application/rss+xml" title="RSS Feed for my blog" href="/feeds/rss" />
    <link rel="alternate" type="application/atom+xml" title="Atom Feed for my blog" href="/feeds/atom" />

    <!-- SEO -->
    <meta property="og:site_name" content="My Blog" />
    <meta property="og:locale" content="en_GB" />
    <meta name="twitter:site" content="@@my-handle" />
    @RenderSection("SeoMeta")
    <meta name="copyright" content="Me">
    <meta name="robots" content="index,follow">

    <title>@ViewBag.Title - Como bajar de peso - Consejos y tips</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="/images/favicon/favicon.ico">
    <link rel="apple-touch-icon" sizes="144x144" type="image/x-icon" href="/images/favicon/apple-touch-icon.png">

    <!-- All CSS Plugins -->
    <link rel="stylesheet" type="text/css" href="/css/plugin.css">

    <!-- Main CSS Stylesheet -->
    <link rel="stylesheet" type="text/css" href="/css/style.css">

    <!-- Google Web Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:400,300,500,600,700">

    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({
            google_ad_client: "ca-pub-4369806819199419",
            enable_page_level_ads: true
        });
    </script>

    @RenderSection("HeadLinks")

    <!-- HTML5 shiv and Respond.js support IE8 or Older for HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Preloader Start -->
    <div class="preloader">
        <div class="rounder"></div>
    </div>
    <!-- Preloader End -->

    <a class="sr-only" href="#content">Saltarse al contenido principal</a>

    <div id="main">
        <div class="container">
            <div class="row">

                <!-- About Me (Left Sidebar) Start -->
                <div class="col-md-3">
                    <div class="about-fixed">

                        <div class="my-pic">
                            @if (!string.IsNullOrEmpty((((Author)ViewBag.Author).ProfileImage)))
                            {
                                <img src="@(((Author)ViewBag.Author).ProfileImage)" alt="">
                            }
                            <a href="javascript:void(0)" class="collapsed" data-target="#menu" data-toggle="collapse"><i class="icon-menu menu"></i></a>
                            <div id="menu" class="collapse">
                                <ul class="menu-link">
                                    <li><a href="/">Inicio</a></li>
                                </ul>
                            </div>
                        </div>

                        <div class="my-detail">

                            <div class="white-spacing">
                                <h1>@(((Author)ViewBag.Author).FirstName) @(((Author)ViewBag.Author).LastName)</h1>
                                <span>@(((Author)ViewBag.Author).Title)</span>
                            </div>

                            <ul class="social-icon">
                                <li><a href="@(((Author) ViewBag.Author).FacebookUrl)" target="_blank" class="facebook"><i class="fab fa-facebook-f"></i></a></li>
                                <li><a href="https://twitter.com/@(((Author)ViewBag.Author).TwitterHandle.TrimStart('@'))" target="_blank" class="twitter"><i class="fab fa-twitter"></i></a></li>
                                <li><a href="@(((Author) ViewBag.Author).LinkedinUrl)" target="_blank" class="linkedin"><i class="fab fa-linkedin-in"></i></a></li>
                                @*                                <li><a href="#" target="_blank" class="github"><i class="fa fa-github"></i></a></li>*@
                            </ul>

                        </div>

                        <div class="categories">
                            <div class="white-spacing">
                                <h3>Categories</h3>
                                @Html.Partial("_Categories", (IEnumerable<Category>)ViewBag.Categories)
                            </div>
                        </div>

                        <div class="tags">
                            <div class="white-spacing">
                                <h3>Etiquetas</h3>
                                @Html.Partial("_TagCloud", (IEnumerable<Tag>)ViewBag.Tags)
                            </div>
                        </div>
                    </div>
                </div>
                <!-- About Me (Left Sidebar) End -->
                <!-- Blog Post (Right Sidebar) Start -->
                <div class="col-md-9">
                    <div class="col-md-12 page-body">
                        <div class="row">
                            <div class="sub-title">
                                <h2>Como bajar de peso - Consejos y tips</h2>
                            </div>

                            <a name="content"></a>
                            <div class="col-md-12 content-page">
                                @RenderBody()
                            </div>

                        </div>

                    </div>


                    <!-- Footer Start -->
                    <div class="col-md-12 page-body margin-top-50 footer">
                        <footer>
                            <ul class="menu-link">
                                <li><a href="/">Inicio</a></li>
                            </ul>

                            <p>© Proyectos Innovadores EIRL @DateTime.Now.Year. Todos los derechos reservados</p>

                            <!-- UiPasta Credit Start -->
                            <div class="uipasta-credit">
                                Diseñado por <a href="http://www.uipasta.com" target="_blank">Proyectos Innovadores EIRL</a> | Contenido desarrollado por <a href="https://buttercms.com/" target="_blank">ButterCMS</a>
                            </div>
                            <!-- UiPasta Credit End -->
                        </footer>
                    </div>
                    <!-- Footer End -->


                </div>
                <!-- Blog Post (Right Sidebar) End -->

            </div>
        </div>
    </div>

    <!-- Back to Top Start -->
    <a href="#" class="scroll-to-top"><i class="fa fa-long-arrow-up"></i></a>
    <!-- Back to Top End -->
    <!-- All Javascript Plugins -->
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <script type="text/javascript" src="/js/plugin.js"></script>

    <!-- Main Javascript File -->
    <script type="text/javascript" src="/js/scripts.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
